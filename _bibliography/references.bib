  shorttitle = {{\emph{Sonic }}{{{\emph{Cradle}}}}},
  booktitle = {Proceedings of the {{Designing Interactive Systems Conference}} on - {{DIS}} '12},
  author = {Vidyarthi, Jay and Riecke, Bernhard E. and Gromala, Diane},
