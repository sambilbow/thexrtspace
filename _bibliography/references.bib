@article{chng2017,
  title = {Shift-{{Life Interactive Art}}: {{Mixed}}-{{Reality Artificial Ecosystem Simulation}}},
  shorttitle = {Shift-{{Life Interactive Art}}},
  author = {Ch'ng, Eugene and Harrison, Dew and Moore, Samantha},
